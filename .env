# Application Settings
APP_NAME=Land Suitability Analysis AI
VERSION=1.0.0
DEBUG=true
HOST=0.0.0.0
PORT=8000

# Database Settings
DATABASE_URL=sqlite:///./land_analysis.db

# Security Settings
SECRET_KEY=your-secret-key-change-in-production-make-it-very-long-and-random
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# External API Keys (Optional - add your own keys)
# GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
# OPENWEATHER_API_KEY=your_openweather_api_key_here
# CRIME_DATA_API_KEY=your_crime_data_api_key_here

# AI Model Settings
MODEL_UPDATE_INTERVAL_DAYS=30
PREDICTION_CONFIDENCE_THRESHOLD=0.7

# Data Collection Settings
DATA_COLLECTION_INTERVAL_HOURS=24
MAX_CONCURRENT_REQUESTS=10

# Redis Settings (for production)
REDIS_URL=redis://localhost:6379/0

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/app.log